generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model GameDouble {
  id        Int      @id @default(autoincrement())
  color     String
  hex       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("gameDouble")
}

model GameCrash {
  id        Int      @id @default(autoincrement())
  crashedIn String
  hex       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("gameCrash")
}
